  "license": "MIT",
  "scripts": {
    "postgres-download": "docker run --name docker-postgres -p 5432:5432 -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=postgres -d postgres",
    "postgres-start": "docker start docker-postgres",
    "postgres-stop": "docker stop docker-postgres",
    "all-test": "npx nx run-many --target=test --coverage",
    "fantasia-angular-serve": "npx nx serve fantasia-angular",
    "fantasia-angular-build": "npx nx build fantasia-angular",
    "fantasia-angular-lint": "./node_modules/.bin/nx workspace-lint && npx nx lint fantasia-angular",
    "fantasia-angular-e2e": "npx nx fantasia-angular-e2e",
    "taskmaster-boot-run": "cd ../middleware/taskmaster && ./gradlew bootRun",
    "taskmaster-boot-run-jar": "java -jar ../middleware/taskmaster/build/libs/*.jar",
    "taskmaster-boot-package": "cd ../middleware/taskmaster && ./gradlew bootJar",
    "taskmaster-boot-image": "cd ../middleware/taskmaster && docker build -t taskmaster-spring-boot .",
    "taskmaster-angular-serve": "npx nx serve taskmaster-angular",
    "taskmaster-angular-build": "npx nx build taskmaster-angular",
    "taskmaster-angular-lint": "./node_modules/.bin/nx workspace-lint && npx nx lint taskmaster-angular",
    "taskmaster-angular-e2e": "npx nx taskmaster-angular-e2e",
    "taskmaster-docker-build": "cd ../docker && docker compose build",
    "taskmaster-docker-run": "cd ../docker && docker compose --env-file .env-local up -d",
    "taskmaster-docker-stop": "cd ../docker && docker compose down",
    "taskmaster-docker-shell-api": "docker exec -it taskmaster-api sh",
    "taskmaster-docker-shell-db": "docker exec -it taskmaster-db sh",
    "taskmaster-docker-shell-fe": "docker exec -it taskmaster-fe sh"
  },
