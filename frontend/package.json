{
  "name": "@angular/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postgres-download": "docker run --name docker-postgres -p 5432:5432 -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=postgres -d postgres",
    "postgres-start": "docker start docker-postgres",
    "postgres-stop": "docker stop docker-postgres",
    "boot-run": "cd ../middleware/taskmaster && ./gradlew bootRun",
    "boot-run-jar": "java -jar ../middleware/taskmaster/build/libs/*.jar",
    "boot-package": "cd ../middleware/taskmaster && ./gradlew bootJar",
    "boot-image": "cd ../middleware/taskmaster && docker build -t taskmaster-spring-boot .",
    "angular-serve": "npx nx serve taskmaster-angular",
    "docker-build": "cd ../docker && docker compose build",
    "docker-run": "cd ../docker && docker-compose --env-file .env-local up -d",
    "docker-stop": "cd ../docker && docker-compose down",
    "docker-shell-api": "docker exec -it taskmaster-api sh",
    "docker-shell-db": "docker exec -it taskmaster-db sh",
    "docker-shell-fe": "docker exec -it taskmaster-fe sh"
  },
  "private": true,
