  "scripts": {
    "postgres-download": "docker run --name docker-postgres -p 5432:5432 -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=postgres -d postgres",
    "postgres-start": "docker start docker-postgres",
    "postgres-stop": "docker stop docker-postgres",
    "all:test": "npx nx run-many --target=test --coverage",
    "all:format": "npx nx format:write",
    "all:format:write": "npx nx format:write",
    "all:format:check": "npx nx format:check",
    "all:dep-graph": "npx nx dep-graph",
    "affected:apps": "npx nx affected:apps",
    "affected:libs": "npx nx affected:libs",
    "affected:build": "npx nx affected:build",
    "affected:e2e": "npx nx affected:e2e",
    "affected:test": "npx nx affected:test",
    "affected:lint": "npx nx affected:lint",
    "affected:dep-graph": "npx nx affected:dep-graph",
    "fantasia-angular-serve": "npx nx serve fantasia-angular",
    "fantasia-angular-build": "npx nx build fantasia-angular",
    "fantasia-angular-test": "npx nx test fantasia-angular",
    "fantasia-angular-lint": "npx nx workspace-lint && npx nx lint fantasia-angular",
    "fantasia-angular-e2e": "npx nx fantasia-angular-e2e",
    "taskmaster-boot-run": "npx nx run taskmaster-java:bootRun",
    "taskmaster-boot-run-jar": "java -jar apps/taskmaster-java/build/libs/*.jar",
    "taskmaster-boot-package": "npx nx run taskmaster-java:bootJar",
    "taskmaster-boot-image": "cd apps/taskmaster-java && docker build -t taskmaster-spring-boot .",
    "taskmaster-angular-serve": "npx nx serve taskmaster-angular",
    "taskmaster-docker-build": "docker compose -f docker/taskmaster/docker-compose.yml build",
    "taskmaster-docker-run": "cd docker/taskmaster && docker compose --env-file .env-local up -d",
    "taskmaster-docker-stop": "cd docker/taskmaster && docker compose down",
    "taskmaster-docker-shell-api": "docker exec -it taskmaster-api sh",
    "taskmaster-docker-shell-db": "docker exec -it taskmaster-db sh",
    "taskmaster-docker-shell-fe": "docker exec -it taskmaster-fe sh",
    "npm:outdated": "npm outdated",
    "nx:update": "npx nx migrate latest",
    "nx:update:check": "npx nx migrate",
    "nx:help": "npx nx help"
  },
